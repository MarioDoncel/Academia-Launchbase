<div class="item">
                <div>Avatar URL</div>
                <div><input type="url" name="avatar_url" id="" placeholder="http://" value="{{instructor.avatar_url}}"></div>
            </div>
            <div class="item">
                <div>Instrutor</div>
                <div><input type="text" name="name" id="" placeholder="Nome do Instrutor" value="{{instructor.name}}"></div>
            </div>
            <div class="item">
                <div>Data de Nascimento</div>
                <div><input type="date" name="birth" value="{{instructor.birth}}"></div>
            </div>
            <div class="item">
                <div>Sexo</div>
                <div>
                    <span><input type="radio" name="gender" value="M" checked {%if instructor.gender == "M" %}checked{%endif%}>MASCULINO</span>
                    <span><input type="radio" name="gender" value="F" {%if instructor.gender == "F" %}checked{%endif%}>FEMININO</span>
                </div>
            </div>
            <div class="item">
                <div>Area de Atuação</div>
                <div>
                    <input type="text" name="services" placeholder="Separe os serviços prestados por ',' " value="{{instructor.services}}">
                </div>
            </div>
            <button type="submit">Salvar</button>